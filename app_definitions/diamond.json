{
	"dockerimage" : "skyport/diamond:latest",
	"commands" : {
		"search" : {
			"blastx" : {
				"input" : [
						   {
						   "type"	: "file",
						   "name"	: "inputfile"
						   },
						   {
						   "type"	: "file",
						   "name"	: "dbfile",
						   "cache" : true
						   },
						   {
						   "type"	: "string",
						   "name"	: "job_id"
						   }
						   ],
				"cmd_script" :
				[
				 "diamond blastx -d ${dbfile} -q ${inputfile} -o ${job_id}.diamond --threads ${NumCPU}"
				 ],
				"outputs" : [
					{
					"name" : "result",
					"filename" : "${job_id}.diamond"
					}
				]
			}
		}
		
	}
}